{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Full boilerplate for React projects.",
  "main": "main.tsx",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint ./src/**/**.tsx",
    "lint:fix": "eslint ./src/**/**.tsx --fix",
    "test": "cypress run --config-file cypress.json"
  },
  "keywords": [
    "react",
    "reactjs",
    "javascript",
    "typescript",
    "cypress",
    "starter",
    "boilerplate"
  ],
  "author": "Krzysztof Bujak",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^1.1.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "cypress": "^6.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
